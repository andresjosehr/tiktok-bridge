{
  "name": "garrys-tiktok",
  "version": "1.0.0",
  "description": "TikTok Live events bridge for Garry's Mod",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "lint": "eslint src/",
    "test": "jest",
    "migrate": "node src/cli/migrate.js",
    "migrate:rollback": "node src/cli/migrate.js --rollback",
    "migrate:status": "node src/cli/migrate.js --status",
    "migrate:fresh": "node src/cli/migrate.js --fresh",
    "make:migration": "node src/cli/make-migration.js",
    "queue:work": "node src/cli/queue-worker.js",
    "queue:clear": "node src/cli/queue-clear.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "mysql2": "^3.6.0",
    "node-cron": "^3.0.2",
    "puppeteer": "^24.15.0",
    "rcon": "^1.1.0",
    "rcon-srcds": "^2.1.0",
    "selenium-webdriver": "^4.34.0",
    "sequelize": "^6.35.0",
    "tiktok-live-connector": "^1.1.6",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "tiktok",
    "garrys-mod",
    "live-streaming",
    "events"
  ],
  "author": "Your Name",
  "license": "MIT"
}
